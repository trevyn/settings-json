{
 "workbench.activityBar.visible": false,
 "editor.accessibilitySupport": "off",
 "editor.minimap.enabled": false,
 "editor.fontSize": 18,
 "terminal.integrated.fontSize": 16,
 // "rust-analyzer.diagnostics.disabled": ["missing-unsafe"],
 "prettier.tabWidth": 1,
 "editor.tabSize": 1,
 "editor.detectIndentation": false,
 "npm.scriptExplorerAction": "run",
 "workbench.editor.pinnedTabSizing": "shrink",
 "editor.quickSuggestions": {
  "other": true,
  "comments": false,
  "strings": true
 },
 "css.lint.unknownAtRules": "ignore",
 "diffEditor.wordWrap": "on",
 "npm.enableRunFromFolder": true,
 "[yaml]": {
  "editor.formatOnSave": false
 },
 "terminal.integrated.scrollback": 10000,
 "editor.wordWrap": "on",
 "editor.wrappingIndent": "indent",
 "[javascript]": {
  "editor.defaultFormatter": "esbenp.prettier-vscode"
 },
 "[html]": {
  "editor.defaultFormatter": "esbenp.prettier-vscode"
 },
 "[jsonc]": {
  "editor.defaultFormatter": "esbenp.prettier-vscode"
 },
 "[json]": {
  "editor.defaultFormatter": "esbenp.prettier-vscode"
 },
 "crates.errorDecorator": "❗️",
 "extensions.ignoreRecommendations": true,
 "extensions.autoUpdate": false,
 "update.mode": "manual",
 "telemetry.enableTelemetry": false,
 "telemetry.enableCrashReporter": false,
 "rust-analyzer.lens.implementations": false,
 "rust-analyzer.lens.enable": true,
 "javascript.suggest.autoImports": false,
 "svelte.plugin.html.tagComplete.enable": false,
 "versionlens.suggestions.showOnStartup": true,
 "svelte.enable-ts-plugin": true,
 "editor.formatOnSave": true,
 "rust-analyzer.inlayHints.enable": false,
 "editor.folding": false,
 "editor.glyphMargin": false,
 "workbench.colorCustomizations": {
  "editorLineNumber.foreground": "#404040",
  "editor.background": "#0f1117",
  "sideBar.background": "#181b22"
 },
 "rust-analyzer.procMacro.enable": false,
 "editor.inlineSuggest.enabled": true,
 "workbench.startupEditor": "none",
 "github.copilot.enable": {
  "*": true,
  "yaml": true,
  "plaintext": true,
  "markdown": true
 },
 "editor.autoClosingQuotes": "never",
 "editor.autoSurround": "never",
 "editor.hover.delay": 1000,
 "editor.hover.sticky": false,
 "window.zoomLevel": -1,
 "rust-analyzer.lens.debug": false
}
